{
    "name": "@koihe/api-type-cli",
    "version": "1.0.0",
    "description": "用于解析Swagger文档，并自动生成API接口类型定义的工具库。",
    "type": "module",
    "main": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "apit": "bin/index.js"
    },
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JY-HE/koihe-api-type-cli.git"
    },
    "homepage": "https://github.com/JY-HE/koihe-api-type-cli",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "build": "rollup --config",
        "publish": "npm publish"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "bin",
        "dist"
    ],
    "keywords": [
        "@koihe",
        "@koihe/api-type-cli",
        "api-type-cli",
        "api",
        "type-cli",
        "swagger"
    ],
    "author": "koihe",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.7.2",
        "cac": "^6.7.14",
        "fs-extra": "^11.2.0",
        "inquirer": "^7.3.3",
        "lodash": "^4.17.21",
        "log-symbols": "^4.1.0",
        "ora": "^5.4.1",
        "path": "^0.12.7",
        "picocolors": "^1.0.0",
        "progress-estimator": "^0.3.1",
        "simple-git": "^3.22.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-replace": "^5.0.7",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/fs-extra": "^5.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/lodash": "^4.17.7",
        "@types/node": "^20.2.1",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^7.32.0",
        "openapi-types": "^7.0.1",
        "rollup": "^3.29.4",
        "tsc-alias": "^1.8.6",
        "typescript": "^4.5.5"
    }
}
