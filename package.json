{
    "name": "@koihe/api-type-cli",
    "version": "1.0.0",
    "description": "用于解析Swagger文档，并自动生成API接口类型定义的工具库。",
    "type": "module",
    "main": "dist/index.mjs",
    "bin": {
        "apit": "bin/index.js"
    },
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JY-HE/koihe-api-type-cli.git"
    },
    "homepage": "https://github.com/JY-HE/koihe-api-type-cli",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
